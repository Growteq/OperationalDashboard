	partition DimDatum-bc886d68-194f-448b-83f2-a4557625f9a8 = calculated
		mode: import
		source = ```
				
				VAR DateTable =
				    ADDCOLUMNS(
				        CALENDAR(
				                DATE( 2019 ,  1 ,  1 )
				            , DATE( 2025 , 4 , 30 )
				        )
				
				    , "Datum"           , FORMAT( [Date] ,  "DD MMM YYYY" )
				    , "DatumGetal"      , FORMAT ( [Date],  "YYYYMMDD" )
				    , "Jaar"            , YEAR  ( [Date] )
				    , "MaandNr"         , MONTH ( [Date] )
				    , "MaandNaamKort"   , FORMAT ( [Date], "mmm" )
				    , "MaandNaamLang"   , FORMAT ( [Date], "mmmm" )
				    , "WeekNr"          , WEEKNUM ( [Date],21 )
				    , "RelatiefWeekNr"  , DATEDIFF( ( [Date] ) , TODAY() , WEEK)
				    , "RelatiefDagNr"   , DATEDIFF( ( [Date] ) , TODAY() , DAY)
				    , "JaarWeekNr"      , YEAR  ( [Date] ) *100 + WEEKNUM ( [Date] ,21)
				    , "JaarWeekNaamKort", "w" & WEEKNUM ( [Date],21 ) &"-"& FORMAT( [Date] ,  "YY" )
				    , "JaarWeekNaam"    , YEAR  ( [Date] ) & "-w" &WEEKNUM ( [Date],21 )            
				    , "WeekNaamKort"    , "w" & WEEKNUM( [Date],21 )
				    , "WeekNaamLang"    , "week " & WEEKNUM( [Date],21 ) 
				    , "WeekdagNr"       , WEEKDAY ( [Date] )
				    , "WeekdagNrRelatief"    , IF(WEEKDAY ( [Date] ) <= WEEKDAY(TODAY()) 
				                                , WEEKDAY([Date]) + WEEKDAY(TODAY())+1
				                                , WEEKDAY([Date]) - WEEKDAY(TODAY()) )
				    , "WeekdagNaamKortRelatief" , FORMAT ( [Date] ,  "ddd" )
				    , "WeekdagNaamKort" , FORMAT ( [Date] ,  "ddd" )
				    , "HuidigWeekNr"    , WEEKNUM(TODAY(),21)
				    , "VorigeWeekLang"      , YEAR  ( Today()-7 ) & "-w" &WEEKNUM ( Today()-7,21 ) 
				    , "WeekdagNaamLang" , FORMAT ( [Date] ,  "dddd" )
				    , "IsYTDweek"       , IF( WEEKNUM ( [Date],21 ) < WEEKNUM ( TODAY(),21) , 1 , 0)
				    , "IsYTD"           , IF(OR(MONTH( [Date]) < Month(TODAY()), AND(MONTH([Date])=MONTH(TODAY()) , DAY([Date]) < DAY(TODAY()))), 1 , 0)
				    , "IsFYTD"          , IF(
				                            OR(
				                                if(month([Date]) >= 5 , MONTH([Date]) -4 , MONTH([Date])+ 8) 
				                                < 
				                                if(month(TODAY()) >= 5 , MONTH(TODAY()) -4 , MONTH(TODAY())+ 8)
				                                
				                            , AND(MONTH([Date])=MONTH(TODAY()) , DAY([Date]) < DAY(TODAY()))), 1 , 0)
				    , "IsToekomst"      , If([Date] > TODAY() , 1 , 0)
				    , "IsHuidigJaar"    ,  IF(YEAR([Date])=Year(TODAY()),1,0)
				    , "IsHuidigeWeek"   , IF(Weeknum([date],21)=WEEKNUM(Today(),21),1,0)
				    , "IsVorigeWeek"    , IF(Weeknum([date],21)=WEEKNUM(Today(),21)-1,1,0)
				    , "BoekjaarNr"      , if(month([Date]) >= 5 , YEAR([Date]) + 1 , YEAR([Date]))
				    , "BoekjaarNaam"    , if(month([Date]) >= 5 , YEAR([Date]) & "-"& (YEAR([Date]) + 1) , (YEAR([Date]) -1 & "-"&YEAR([Date])))
				    , "BoekmaandNr"     , if(month([Date]) >= 5 , MONTH([Date]) -4 , MONTH([Date])+ 8)
				    , "BoekmaandNaamKort"   , FORMAT ( [Date], "mmm" )
				    , "BoekmaandNaamLang"   , FORMAT ( [Date], "mmmm" )
				    , "IsVandaag"        , if([Date] = TODAY() , 1 , 0)
				    ,  "Iteratie", IF(
				            ISEVEN( WEEKNUM([Date],21))
				            , WEEKNUM([Date],21)-1 & "-" & WEEKNUM([Date],21)
				            ,WEEKNUM([Date],21) & "-"  & WEEKNUM([Date],21)+1 
				            )
				    , "Jaar Iteratie", IF(
				            ISEVEN( WEEKNUM([Date],21))
				            ,YEAR  ( [Date] ) & "/" & WEEKNUM([Date],21)-1 & "-" & WEEKNUM([Date],21)
				            ,YEAR  ( [Date] ) & "/" & WEEKNUM([Date],21) & "-"  & WEEKNUM([Date],21)+1 
				            ) 
				    , "IteratieStartWeek"   , IF(
				            ISEVEN( WEEKNUM([Date],21))
				            , WEEKNUM([Date],21)-1 
				            ,WEEKNUM([Date],21) 
				            )
				    ,"IteratieEindWeek"   , IF(
				            ISEVEN( WEEKNUM([Date],21))
				            , WEEKNUM([Date],21)
				            ,WEEKNUM([Date],21)+1 
				            )
				    )
				
				VAR DateTable2 = ADDCOLUMNS(
				    DateTable
				    ,    "IsHuidigeIteratie", IF(Weeknum(Today(),21) in {[IteratieStartWeek],[IteratieEindWeek]},1,0)
				    ,   "VolgendeIteratie", [IteratieStartWeek]+2 & "-" & [IteratieEindWeek] + 2
				    ,   "VorigeIteratie", [IteratieStartWeek]-2 & "-" & [IteratieEindWeek] - 2
				    ,   "IsHuidigeMaand" , if(MONTH(today()) = [MaandNr],1,0)
				)
				
				RETURN DateTable2
				```

