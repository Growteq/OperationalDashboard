        "typeProperties": {
          "source": {
            "type": "DataWarehouseSource",
            "sqlReaderQuery": "WITH DeduplicatedRooster AS (\r\n    SELECT \r\n        m.Medewerker,\r\n        MAX(Aantal_uren_per_week) AS Aantal_uren_per_week -- Kies de hoogste waarde als er meerdere zijn\r\n    FROM \r\n        [s2_stg_afas].[Rooster] m\r\n    GROUP BY \r\n        m.Medewerker\r\n)\r\nSELECT \r\n    ROW_NUMBER() OVER (ORDER BY CAST(mw.Datum_in_dienst AS DATE), CAST(mw.Datum_uit_dienst AS DATE)) AS InUitStroomKey, -- Genereer unieke sleutel\r\n    mw.Medewerker AS UniekeMedewerker,\r\n    mw.Naam AS MedewerkerNaam,\r\n    CAST(mw.Datum_in_dienst AS DATE) AS DatumInstroom, -- Instroomdatum als DATE\r\n    CAST(mw.Datum_uit_dienst AS DATE) AS DatumUitstroom, -- Uitstroomdatum als DATE\r\n    CASE \r\n        WHEN mw.Datum_in_dienst IS NOT NULL THEN 1 \r\n        ELSE 0 \r\n    END AS InstroomAantal,\r\n    CASE \r\n        WHEN mw.Datum_in_dienst IS NOT NULL THEN CAST(dr.Aantal_uren_per_week / 40.0 AS DECIMAL(6, 2))\r\n        ELSE 0\r\n    END AS InstroomFTE,\r\n    CASE \r\n        WHEN mw.Datum_uit_dienst IS NOT NULL THEN 1\r\n        ELSE 0\r\n    END AS UitstroomAantal,\r\n    CASE \r\n        WHEN mw.Datum_uit_dienst IS NOT NULL THEN CAST(dr.Aantal_uren_per_week / 40.0 AS DECIMAL(6, 2))\r\n        ELSE 0\r\n    END AS UitstroomFTE,\r\n    m3.MedewerkerKey,\r\n    m3.TeamKey,\r\n    t3.PracticeKey\r\nFROM \r\n    [s2_stg_afas].[Medewerkergegevens] mw\r\nLEFT JOIN \r\n    DeduplicatedRooster dr\r\nON \r\n    mw.Medewerker = dr.Medewerker\r\nLEFT JOIN \r\n    s3_ods.DimMedewerkers m3\r\nON \r\n    mw.Medewerker = m3.MedewerkerId\r\nLEFT JOIN \r\n    s3_ods.DimTeam t3\r\nON\r\n    m3.TeamKey = t3.TeamKey\r\nORDER BY \r\n    DatumInstroom, DatumUitstroom",
