        "typeProperties": {
          "source": {
            "type": "DataWarehouseSource",
            "sqlReaderQuery": "WITH DatumFiltered AS (\n    SELECT *\n    FROM [s3_ods].[DimDatum]\n    WHERE Datum >= '2025-01-01'\n),\nVerlofbron AS (\n    SELECT \n        [Medewerker],\n        [Naam],\n        [Type_verlof],\n        [Type_verlof_code],\n        CAST([Begindatum_-tijd] AS DATE) AS Begindatum,\n        CAST([Einddatum_-tijd] AS DATE) AS Einddatum,\n        [Omschrijving],\n        TRY_CAST([Aantal_uren] AS DECIMAL(10,2)) AS Aantal_uren,\n        [Datum_aangevraagd]\n    FROM [s2_stg_afas].[Verlofboeking]\n    WHERE 0=0\n      AND Type_verlof_code != 'WV2025'\n      AND [Begindatum_-tijd] >= '2025-01-01'\n      AND TRY_CAST([Aantal_uren] AS DECIMAL(10,2)) IS NOT NULL\n),\nVerlofPerDag AS (\n    SELECT \n        d.Datum,\n        d.WeekDagNaam,\n        v.Medewerker,\n        v.Naam,\n        v.Type_verlof,\n        v.Type_verlof_code,\n        v.Omschrijving,\n        v.Begindatum,\n        v.Einddatum,\n        v.Datum_aangevraagd,\n        v.Aantal_uren,\n        DATEDIFF(DAY, v.Begindatum, v.Einddatum) + 1 AS AantalDagen\n    FROM DatumFiltered d\n    JOIN Verlofbron v\n        ON d.Datum BETWEEN v.Begindatum AND v.Einddatum\n),\nMetPrioriteit AS (\n    SELECT *,\n        ROW_NUMBER() OVER (\n            PARTITION BY Datum, Medewerker\n            ORDER BY \n                CASE \n                    WHEN Type_verlof_code = 'F' THEN 1\n                    WHEN Type_verlof_code LIKE 'V%' THEN 2\n                    ELSE 3\n                END\n        ) AS Prioriteit\n    FROM VerlofPerDag\n),\nGefilterdVerlof AS (\n    SELECT *\n    FROM MetPrioriteit\n    WHERE Prioriteit = 1\n),\nMetRoosterUren AS (\n    SELECT \n        gv.*,\n        fr.UrenPerDag\n    FROM GefilterdVerlof gv\n    LEFT JOIN [s3_ods].[FeitRoosterUren] fr\n        ON gv.Datum = fr.Datum\n        AND gv.Medewerker = fr.Medewerker\n        AND gv.Naam = fr.Naam\n),\nResultaat AS (\n    SELECT \n        ROW_NUMBER() OVER (ORDER BY Datum, Medewerker, Type_verlof_code) AS FeitVerlofAanvraagKey,\n        Datum,\n        WeekDagNaam,\n        Medewerker,\n        Naam,\n        Type_verlof,\n        Type_verlof_code,\n        Omschrijving,\n        Begindatum,\n        Einddatum,\n        Datum_aangevraagd,\n        ROUND(\n            CASE \n                WHEN Begindatum = Einddatum THEN Aantal_uren / 60.0\n                ELSE UrenPerDag\n            END, 2\n        ) AS Aantal_uren\n    FROM MetRoosterUren\n),\nMetMedewerkerKey AS (\n    SELECT \n        r.*,\n        dm.MedewerkerKey\n    FROM Resultaat r\n    LEFT JOIN [s3_ods].[DimMedewerkers] dm\n        ON r.Medewerker = dm.MedewerkerId\n)\nSELECT *\nFROM MetMedewerkerKey\nORDER BY Datum\n",
            "queryTimeout": "02:00:00",
            "partitionOption": "None",
