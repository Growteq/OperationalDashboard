table TimeIntelligence
	lineageTag: 3e2f30d9-4c8c-4899-a624-564284b10cad

	calculationGroup

		calculationItem 'Komende 12 maanden' =
				
				VAR MndMax = [vHMnrRel] +12
				VAR MndMin = [vHMnrRel]
				RETURN
				CALCULATE(SELECTEDMEASURE()
				, KEEPFILTERS(FILTER(ALL(DimDatum), DimDatum[MaandNrRelatief] >= MndMin && DimDatum[MaandNrRelatief] <= MndMax))
				, all(DimDatumFilter)
				)

		calculationItem 'Laatste 12 maanden' =
				
				VAR MndMax = [vHMnrRel]
				VAR MndMin = MndMax - 11
				RETURN
				CALCULATE(SELECTEDMEASURE()
				, KEEPFILTERS(FILTER(ALL(DimDatum), DimDatum[MaandNrRelatief] >= MndMin && DimDatum[MaandNrRelatief] <= MndMax))
				, all(DimDatumFilter)
				)

		calculationItem 'TESTKomende 12 maanden' = ```
				
				VAR MndMax = [vHMnrRel]  -- Huidige maand in relatieve telling
				VAR MndMin = MndMax + 12 -- De grens voor de komende 12 maanden
				RETURN
				CALCULATE(
				    SELECTEDMEASURE(),
				    KEEPFILTERS(
				        FILTER(ALL(DimDatum), DimDatum[MaandNrRelatief] >= MndMax && DimDatum[MaandNrRelatief] < MndMin)
				    ),
				    ALL(DimDatumFilter)
				)
				
				```

		calculationItem 'Voorgaande 12 maanden' =
				
				VAR MndMax = [vHMnrRel]
				VAR MndMin = MndMax - 11
				RETURN
				CALCULATE(
				  CALCULATE(  SELECTEDMEASURE() , SAMEPERIODLASTYEAR(DimDatum[Datum]))
				, KEEPFILTERS(FILTER(ALL(DimDatum), DimDatum[MaandNrRelatief] >= MndMin && DimDatum[MaandNrRelatief] <= MndMax))
				, all(DimDatumFilter)
				)

